#4544
if iscombat $n
  if cabal $n 'warmaster'
    obj echoat $n 'This is not the way a Warmaster should act!'
    break
  endif
else
  obj echoat $n You must be in combat first.
  break
endif
if isdelay $i
  obj echoat $n $I's powers are drained.
  break
endif
if race $f fire
  if objval3 $i == 30
    break
  endif
  obj echo $I begins to smoke with cold!
  obj attrib none v3 = 30
  obj delay 4
  break
endif
if race $f 'Halfling'
  if objval3 $i == 7
    break
  endif
  obj echo $I loses all traces of magic.
  obj attrib none v3 = 7
  obj delay 4
  break
endif
if race $f 'dwarf'
or race $f 'duergar'
  if objval3 $i == 72
    break
  endif
  obj echo $I begins to spray torrents of water!
  obj attrib none v3 = 72
  obj delay 4
  break
endif
if race $f 'illithid'
or race $f 'vampire'
  if objval3 $i == 52
    break
  endif
  obj echo $I begins to emit blinding rays of light!
  obj attrib none v3 = 52
  obj delay 4
  break
endif
if race $f 'undead'
  if objval3 $i == 18
    break
  endif
  obj echo $I begins to glow with a holy aura!
  obj attrib none v3 = 18
  obj delay 4
  break
endif
if objval3 $i != 19
  obj echo $I begins to hum with magic!
  obj attrib none v3 = 19
  obj delay 4
  break
endif
