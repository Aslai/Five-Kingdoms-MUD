  break
endif
if level $n < 31
  obj echoat $n `@You gain 5000 experience, 10000 gold, 500 cps and few practices!``
  obj gain exp pc $n 5000 5000
  obj gain gold pc $n 10000 10000  
  obj gain cp pc $n 500 500
  obj gain prac pc $n 4 6
else
  obj echoat $n `@You gain 10000 gold, 500 cps and few practices!``
  obj gain gold pc $n 10000 10000  
  obj gain cp pc $n 500 500
  obj gain prac pc $n 4 6
